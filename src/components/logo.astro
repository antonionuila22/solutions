---
import logo from "../assets/codebrand.svg";
const { sizeW, sizeH } = Astro.props;
---

<a href="/" class="flex-shrink-0" aria-label="Codebrand - Go to homepage"
  ><img
    class="w-32 sm:w-36"
    width="144"
    height="40"
    src={logo.src}
    width={sizeW}
    height={sizeH}
    alt="Astro Homepage"
  /></a
>
